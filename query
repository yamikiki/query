CREATE TABLE user (
  username varchar(12) NOT NULL,
  password varchar(30) DEFAULT NULL,
  email varchar(30) DEFAULT NULL,
  profilePic varchar(100) DEFAULT NULL,
  PRIMARY KEY (username),
  UNIQUE KEY username_UNIQUE (username);
) 

